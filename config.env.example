# Startup
ON_START_MODE=rebalance   # only use rebalance

# Auto TP/SL
TP_BPS=150          # +1.5% from avg entry
SL_BPS=80           # -0.8% from avg entry
USE_TRAILING=true
TRAIL_BPS=100       # -1.0% from trailing high

# Keep your existing values for these
PRICE_POLL_SEC=5
DECISION_MIN=1
TRADE_COOLDOWN_SEC=20
DRY_RUN=true

# Add your Recall Network API key here
RECALL_API_KEY=your_api_key_here
